<types namespace="_dropcss">
  <type name="Config" desc="Options for the program.">
    <prop string name="html">
      The input HTML.
    </prop>
    <prop string name="css">
      The CSS to drop selectors from.
    </prop>
    <prop boolean name="shouldDrop" default="false">
      Whether _DropCSS_ should remove selectors.
    </prop>
  </type>
</types>