{
  "name": "@lemuria/dropcss",
  "version": "0.0.0-pre",
  "description": "[fork] An exceptionally fast, thorough and tiny unused-CSS cleaner.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "dev": "src/bin/index.js",
    "wiki": "NODE_DEBUG=doc doc doc-wiki -W wiki -n _dropcss",
    "compile": "depack src/bin/dropcss -c -a -o compile/bin --externs types/externs.js",
    "lib": "depack src/depack -c -a -o compile/dropcss.js",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _dropcss",
    "d": "yarn-s template d2 externs",
    "d2": "typal types/index.js compile/index.js -t types/index.xml -u",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "e": "alanode",
    "args": "argufy types/arguments.xml -o src/bin/args.js"
  },
  "bin": "compile/bin/dropcss.js",
  "files": [
    "compile",
    "src",
    "types/externs.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/kumarikandam/dropcss.git"
  },
  "keywords": [
    "dropcss",
    "lemuria"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumarikandam/dropcss/issues"
  },
  "homepage": "https://github.com/kumarikandam/dropcss#readme",
  "devDependencies": {
    "alamode": "^3.2.0",
    "argufy": "^1.7.1",
    "depack": "^1.0.0",
    "documentary": "^1.34.1",
    "eslint-config-artdeco": "1.0.1",
    "temp-context": "^2.1.3",
    "usually": "^1.2.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.2"
  }
}
